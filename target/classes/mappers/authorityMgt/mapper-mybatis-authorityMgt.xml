<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.mycompany.app.authority.authorityMapper">

    <select id="getAuthorityList"
        resultType="com.mycompany.app.authority.model.BoardVO">
        SELECT
        name, empNo, comp, dept, proleId
